{
  "version": 2,
  "builds": [
    {
      "src": "src/server.ts",
      "use": "@vercel/node",
      "config": { "includeFiles": ["dist/**",".env"] }
    }
  ],
  "routes": [
    {
      "src": "/(.*)",
      "dest": "/dist/server.js",
      "methods": ["GET", "POST", "PUT", "PATCH", "DELETE", "OPTIONS"]
    }
  ]
}
